<%= form_for @ticket_type do |f| %>
  <div class="form-group">
    <%= f.label :name  %>
    <%= f.text_field :name %>
  </div>
  
  <div class="form-group">
    <%= f.label :price  %>
    <%= f.text_field :price %>
  </div>
  
  <div class="form-group">
    <%= f.label :max_quantity %>
    <%= f.text_field :max_quantity %>
  </div>
    <%= f.text_field :event_id, value: @event.id, type: "hidden" %>
  
    <%= f.submit %>
  

<% end %>
